{
  "name": "technically",
  "version": "1.0.0",
  "description": "Get the “exact value” of the number",
  "main": "index.js",
  "scripts": {
    "mochify:node": "mochify \"test/*.ls\" --reporter spec --extension .ls --transform browserify-livescript --node",
    "mochify:phantom": "mochify \"test/*.ls\" --reporter spec --extension .ls --transform browserify-livescript",
    "test": "npm run mochify:node && npm run mochify:phantom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hakatashi/exact-number.git"
  },
  "keywords": [
    "number",
    "toString",
    "ieee754",
    "IEEE 754",
    "convert",
    "floating point"
  ],
  "author": "Koki Takahashi <hakatasiloving@gmail.com> (http://hakatashi.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hakatashi/exact-number/issues"
  },
  "homepage": "https://github.com/hakatashi/exact-number#readme",
  "devDependencies": {
    "browserify-livescript": "^0.2.3",
    "chai": "^3.5.0",
    "livescript": "^1.5.0",
    "mocha": "^3.0.2",
    "mochify": "^2.18.1",
    "mochify-istanbul": "^2.4.1"
  },
  "dependencies": {
    "big.js": "^3.1.3",
    "ieee754": "^1.1.6",
    "left-pad": "^1.1.1"
  }
}
